# Docker Compose project name
COMPOSE_PROJECT_NAME=usecallmanager

# Development mode (enables hot-reload)
DEV_MODE=false

# Domain for caddy-docker-proxy
SERVICES_DOMAIN=services.local

# CGI Authentication
SERVICES_CGI_USERNAME=cisco
SERVICES_CGI_PASSWORD=cisco

# Asterisk Manager Interface
SERVICES_MANAGER_URL=http://asterisk:8088/mxml
SERVICES_MANAGER_USERNAME=asterisk
SERVICES_MANAGER_SECRET=asterisk

# Reports directory (inside container)
SERVICES_REPORTS_DIR=/var/log/cisco
